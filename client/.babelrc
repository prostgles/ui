{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "esmodules": true
        },
        "modules": false,
        "bugfixes": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": ["dynamic-import-node"],
  "env": {
    "production": {
      "presets": [
        [
          "minify",
          {
            "builtIns": false,
            "evaluate": false,
            "mangle": false
          }
        ]
      ]
    }
  }
}
