  "scripts": {
    "prod": "NODE_ENV=production BABEL_ENV=production webpack",
    "devOld": "NODE_ENV=development BABEL_ENV=development webpack --stats none --watch  ",
    "start": "node scripts/start.js --no-cache",
    "build-start": "rm -rf ./build && webpack --config=configs/prod.js && npm prune --omit=dev",
    "build": "bash ./build.sh",
    "dev": "npm i --quiet && NODE_OPTIONS='--max-old-space-size=4048' webpack --watch --config=configs/dev.js",
    "lib": "rm -rf ./dist && tsc --project tsconfig_lib.json && npm run copyf",
    "copyf": "cd ./src && find . -name '*.css' -type f -exec cp --parents {} ../dist/ \\; ",
    "copyf2": "find ./src/. -name '*.css' | cpio -pdm ./dist/",
    "lint": "eslint . --ext .ts,.tsx --quiet --fix",
    "tsc": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
